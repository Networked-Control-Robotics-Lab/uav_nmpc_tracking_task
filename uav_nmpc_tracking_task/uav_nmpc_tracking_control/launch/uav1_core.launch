<launch>
   <node pkg="uav_nmpc_tracking_control" name="uav1_core" type="uav1_core" output="screen" >
     <param name="topic_box_uav1" value="/uav1/YOLO/box" />
<!--PID in camera frame-->
	 <param name="KPx_ibvs" value="0.9" />
     <param name="KPy_ibvs" value="0.9" />		<!--pseudo inverse of Le=3x6, Kpy_ibvs=3.0-->
     <param name="KPz_ibvs" value="0.9" />
	 <param name="KProll_ibvs" value="1.2" />
	 <param name="Tix_ibvs" value="10000" />
     <param name="Tiy_ibvs" value="10000" />
	 <param name="Tiz_ibvs" value="10000" />
     <param name="Tiroll_ibvs" value="5" />
     <param name="Tdx_ibvs" value="0" />
     <param name="Tdy_ibvs" value="0" />
	 <param name="Tdz_ibvs" value="0" />
     <param name="Tdroll_ibvs" value="0.8" />

     <param name="P_init_0" value="0.1" />
     <param name="P_init_1" value="0.1" />
     <param name="P_init_2" value="0.1" />
	 <param name="P_init_3" value="800" />
     <param name="P_init_4" value="800" />
